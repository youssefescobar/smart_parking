{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/dashboard",
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "index.js"
    }
  ]
}
